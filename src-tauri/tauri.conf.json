{
  "$schema": "..\\node_modules/@tauri-apps/cli\\schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "distDir": "../dist",
    "devPath": "../dist"
  },
  "package": {
